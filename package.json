{
  "name": "@lyrical/effects",
  "version": "0.1.0",
  "description": "Lyrical Effects.",
  "keywords": [
    "typescript",
    "lyrical",
    "effects"
  ],
  "author": {
    "name": "City",
    "email": "city@nanzc.com"
  },
  "homepage": "https://github.com/lyrical-city/effects#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyrical-city/effects.git"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "lint:es": "eslint -c .eslintrc.js --ext .ts,.tsx ./",
    "lint:es-fix": "eslint --fix -c .eslintrc.js --ext .ts,.tsx ./",
    "prepublishOnly": "npm run build",
    "build": "lough-build",
    "dev": "lough-build dev"
  },
  "devDependencies": {
    "@lough/build-cli": "^0.2.0",
    "@lough/commitlint-config": "^0.1.3",
    "@lough/eslint-config": "^0.1.6",
    "@lough/tsconfig": "^0.0.5",
    "@types/jest": "^29.5.3",
    "husky": "^7.0.2",
    "jest": "^29.6.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.js --ext .ts,.tsx"
    ],
    "*.{css,less,scss,styl}": [
      "stylelint --config .stylelintrc.js *.{css,less,scss,styl}"
